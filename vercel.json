{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "silvaneves.com.br"
        }
      ],
      "destination": "https://www.silvaneves.com.br/:1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "protocol",
          "value": "http"
        }
      ],
      "destination": "https://www.silvaneves.com.br/:1",
      "permanent": true
    }
  ]
}
